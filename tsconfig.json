{
  "compilerOptions": {
    "declaration": true,
    "sourceMap": true,
    "strict": true,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "noImplicitReturns": false,
    "noFallthroughCasesInSwitch": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "jsx": "react",
    "baseUrl": ".",
    "skipLibCheck": true,
    "target": "es2017",
    "module": "commonjs"
  }
}
